{
    "preset": "laravel",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "array_indentation": true,
        "array_syntax": {
            "syntax": "short"
        },
        "binary_operator_spaces": {
            "default": "align_single_space_minimal"
        },
        "blank_line_before_statement": {
            "statements": ["return"]
        },
        "ordered_imports": {
            "sort_algorithm": "alpha"
        }
    }
}
