{
    "name": "musonza/chat",
    "description": "Laravel Chat Package",
    "type": "library",
    "keywords": [
        "laravel",
        "chat",
        "messaging",
        "conversation"
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "^5.3",
        "illuminate/config": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.5.*",
        "orchestra/database": "3.5.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tinashe Musonza",
            "email": "tinashemusonza@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Musonza\\Chat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Musonza\\Chat\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Musonza\\Chat\\ChatServiceProvider"
            ]
        }
    }
}